{
  "name": "Hide Elements",
  "version": "1.1",
  "description": "Hide elements using the context menu.",
  "permissions": [
    "contextMenus"
  ],
  "minimum_chrome_version": "6",
  "background_page": "background.html",
  "icons": {
    "16": "edit-delete16.png",
    "32": "edit-delete32.png",
    "48": "edit-delete48.png",
    "128": "edit-delete128.png",
    "512": "edit-delete512.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["hide_listener.js"]
    }
  ]
}
